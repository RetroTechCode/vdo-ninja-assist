{
  "compilerOptions": {
    "composite": true,
    "target": "ES2020",
    "module": "ES2020",
    "outDir": "dist-electron",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["node", "electron"],
    "skipLibCheck": true
  },
  "include": ["electron/**/*"]
}
